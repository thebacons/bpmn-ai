<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_selfHealing" targetNamespace="http://example.com/selfhealing">
  <bpmn:process id="process_selfHealing" name="Deterministic AI Self‑Healing Process" isExecutable="true">
    <bpmn:laneSet id="LaneSet_selfHealing">
      <bpmn:lane id="Lane_Monitoring" name="Monitoring">
        <bpmn:flowNodeRef>EndEvent_NormalOperation</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_AnomalyDetected</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DetectAnomaly</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CollectMetrics</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>StartEvent_CollectMetrics</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_AIEngine" name="AI Engine">
        <bpmn:flowNodeRef>Task_GeneratePlan</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ExecuteRemediation</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_VerifyOutcome</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RemediationSuccessful</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RootCauseAnalysis</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_RemediationNeeded</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AnalyzeAnomaly</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Remediation" name="Remediation Service">
        <bpmn:flowNodeRef>Task_SendRecoveryNotification</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_RecoveryCompleted</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Notification" name="Notification Service">
        <bpmn:flowNodeRef>Task_EscalateIssue</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_EscalationCompleted</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_CollectMetrics" name="Start">
      <bpmn:outgoing>Flow_Start_To_Collect</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_CollectMetrics" name="Collect Metrics">
      <bpmn:incoming>Flow_Start_To_Collect</bpmn:incoming>
      <bpmn:outgoing>Flow_Collect_To_Detect</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_DetectAnomaly" name="Detect Anomaly">
      <bpmn:incoming>Flow_Collect_To_Detect</bpmn:incoming>
      <bpmn:outgoing>Flow_Detect_To_Gateway</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_AnomalyDetected" name="Anomaly Detected?">
      <bpmn:incoming>Flow_Detect_To_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Gateway_To_Normal</bpmn:outgoing>
      <bpmn:outgoing>Flow_Gateway_To_Analyze</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_NormalOperation" name="Normal Operation">
      <bpmn:incoming>Flow_Gateway_To_Normal</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="Task_AnalyzeAnomaly" name="Analyze Anomaly">
      <bpmn:incoming>Flow_Gateway_To_Analyze</bpmn:incoming>
      <bpmn:outgoing>Flow_Analyze_To_RootCause</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_RootCauseAnalysis" name="Root‑Cause Analysis">
      <bpmn:incoming>Flow_Analyze_To_RootCause</bpmn:incoming>
      <bpmn:outgoing>Flow_RootCause_To_RemediationGateway</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_RemediationNeeded" name="Remediation Needed?">
      <bpmn:incoming>Flow_RootCause_To_RemediationGateway</bpmn:incoming>
      <bpmn:outgoing>Flow_RemediationGateway_To_Plan</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_GeneratePlan" name="Generate Remediation Plan">
      <bpmn:incoming>Flow_RemediationGateway_To_Plan</bpmn:incoming>
      <bpmn:outgoing>Flow_Plan_To_Execute</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ExecuteRemediation" name="Execute Remediation">
      <bpmn:incoming>Flow_Plan_To_Execute</bpmn:incoming>
      <bpmn:outgoing>Flow_Execute_To_Verify</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_VerifyOutcome" name="Verify Outcome">
      <bpmn:incoming>Flow_Execute_To_Verify</bpmn:incoming>
      <bpmn:outgoing>Flow_Verify_To_SuccessGateway</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_RemediationSuccessful" name="Remediation Successful?">
      <bpmn:incoming>Flow_Verify_To_SuccessGateway</bpmn:incoming>
      <bpmn:outgoing>Flow_SuccessGateway_To_Notify</bpmn:outgoing>
      <bpmn:outgoing>Flow_SuccessGateway_To_Escalate</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_SendRecoveryNotification" name="Send Recovery Notification">
      <bpmn:incoming>Flow_SuccessGateway_To_Notify</bpmn:incoming>
      <bpmn:outgoing>Flow_Notify_To_EndRecovery</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_RecoveryCompleted" name="Recovery Completed">
      <bpmn:incoming>Flow_Notify_To_EndRecovery</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:task id="Task_EscalateIssue" name="Escalate Issue">
      <bpmn:incoming>Flow_SuccessGateway_To_Escalate</bpmn:incoming>
      <bpmn:outgoing>Flow_Escalate_To_EndEscalation</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_EscalationCompleted" name="Escalation Completed">
      <bpmn:incoming>Flow_Escalate_To_EndEscalation</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Start_To_Collect" sourceRef="StartEvent_CollectMetrics" targetRef="Task_CollectMetrics" />
    <bpmn:sequenceFlow id="Flow_Collect_To_Detect" sourceRef="Task_CollectMetrics" targetRef="Task_DetectAnomaly" />
    <bpmn:sequenceFlow id="Flow_Detect_To_Gateway" sourceRef="Task_DetectAnomaly" targetRef="Gateway_AnomalyDetected" />
    <bpmn:sequenceFlow id="Flow_Gateway_To_Normal" sourceRef="Gateway_AnomalyDetected" targetRef="EndEvent_NormalOperation" />
    <bpmn:sequenceFlow id="Flow_Gateway_To_Analyze" sourceRef="Gateway_AnomalyDetected" targetRef="Task_AnalyzeAnomaly" />
    <bpmn:sequenceFlow id="Flow_Analyze_To_RootCause" sourceRef="Task_AnalyzeAnomaly" targetRef="Task_RootCauseAnalysis" />
    <bpmn:sequenceFlow id="Flow_RootCause_To_RemediationGateway" sourceRef="Task_RootCauseAnalysis" targetRef="Gateway_RemediationNeeded" />
    <bpmn:sequenceFlow id="Flow_RemediationGateway_To_Plan" sourceRef="Gateway_RemediationNeeded" targetRef="Task_GeneratePlan" />
    <bpmn:sequenceFlow id="Flow_Plan_To_Execute" sourceRef="Task_GeneratePlan" targetRef="Task_ExecuteRemediation" />
    <bpmn:sequenceFlow id="Flow_Execute_To_Verify" sourceRef="Task_ExecuteRemediation" targetRef="Task_VerifyOutcome" />
    <bpmn:sequenceFlow id="Flow_Verify_To_SuccessGateway" sourceRef="Task_VerifyOutcome" targetRef="Gateway_RemediationSuccessful" />
    <bpmn:sequenceFlow id="Flow_SuccessGateway_To_Notify" sourceRef="Gateway_RemediationSuccessful" targetRef="Task_SendRecoveryNotification" />
    <bpmn:sequenceFlow id="Flow_SuccessGateway_To_Escalate" sourceRef="Gateway_RemediationSuccessful" targetRef="Task_EscalateIssue" />
    <bpmn:sequenceFlow id="Flow_Notify_To_EndRecovery" sourceRef="Task_SendRecoveryNotification" targetRef="EndEvent_RecoveryCompleted" />
    <bpmn:sequenceFlow id="Flow_Escalate_To_EndEscalation" sourceRef="Task_EscalateIssue" targetRef="EndEvent_EscalationCompleted" />
  </bpmn:process>
